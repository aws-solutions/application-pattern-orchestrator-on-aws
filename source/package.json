{
    "name": "rapm-solution",
    "version": "1.2.0",
    "description": "Reusable Application Patterns Manager Solution",
    "author": {
        "name": "Amazon Web Services",
        "url": "https://aws.amazon.com/solutions"
    },
    "license": "Apache-2.0",
    "scripts": {
        "cleanup": "tsc --build ./ --clean && rm -rf node_modules && rm -f package-lock.json",
        "cleanup:tsc": "tsc --build ./ --clean",
        "zip-blueprint-infrastructure": "cd blueprint-infrastructure && node ../dist/lib/infra-utils/zip-bundle.js && cd ..",
        "build": "tsc && npm run zip-blueprint-infrastructure",
        "watch": "tsc -w",
        "test": "jest --coverage",
        "lint": "eslint --ext .ts . --fix",
        "cdk": "cdk",
        "synth": "cdk context --clear && cdk synth -q",
        "build-lambda": "cd lambda/blueprintgovernanceservice && npm run all && cd ../..",
        "build-blueprint-infrastructure": "cd blueprint-infrastructure && npm ci && npm run build && npm run lint && npm test && cd ..",
        "all": "npm ci && cd blueprint-ui && yarn all && cd .. && npm run build-lambda && npm run build-blueprint-infrastructure && npm run lint && npm run build",
        "deploy": "npm run synth && cdk deploy --all",
        "release:patch": "standard-version --release-as patch",
        "release:minor": "standard-version --release-as minor",
        "release": "standard-version",
        "cy:open": "cypress open",
        "cy:run": "cypress run --browser chrome"
    },
    "devDependencies": {
        "@aws-sdk/client-cloudformation": "^3.264.0",
        "@aws-sdk/client-codeartifact": "^3.264.0",
        "@aws-sdk/client-codecommit": "^3.264.0",
        "@aws-sdk/client-dynamodb": "^3.264.0",
        "@aws-sdk/client-service-catalog": "^3.264.0",
        "@aws-sdk/client-ssm": "^3.264.0",
        "@octokit/rest": "^20.0.1",
        "@types/archiver": "^5.3.1",
        "@types/jest": "^29.1.0",
        "@typescript-eslint/eslint-plugin": "^6.2.0",
        "@typescript-eslint/parser": "^6.2.0",
        "archiver": "^5.3.0",
        "aws-amplify": "^5.0.12",
        "aws-cdk": "^2.85.0",
        "axios": "^1.3.1",
        "cypress": "^12.3.0",
        "dotenv": "^16.0.3",
        "esbuild": "^0.14.51",
        "eslint": "^8.20.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-typescript": "^3.0.0",
        "eslint-plugin-header": "^3.1.1",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-prettier": "^5.0.0",
        "jest": "^29.3.1",
        "prettier": "^3.0.0",
        "standard-version": "^9.3.1",
        "ts-jest": "^29.0.3",
        "typescript": "^5.0.4"
    },
    "dependencies": {
        "@aws-cdk/aws-servicecatalogappregistry-alpha": "^2.85.0-alpha.0",
        "@aws-cdk/aws-synthetics-alpha": "^2.85.0-alpha.0",
        "@types/node": "^20.4.5",
        "aws-cdk-lib": "^2.85.0",
        "aws-sdk": "^2.1354.0",
        "cdk-nag": "^2.15.51",
        "constructs": "^10.1.84",
        "source-map-support": "^0.5.16",
        "winston": "^3.8.1"
    },
    "overrides": {
        "fast-xml-parser": "^4.2.5",
        "semver": "^7.5.3",
        "word-wrap": "^1.2.5",
        "tough-cookie": "^4.1.3"
    }
}
